{
  "name": "@flagdash/sdk",
  "version": "0.1.0",
  "description": "FlagDash client SDK for feature flags, remote config, and AI configs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "feature-flags",
    "remote-config",
    "ai-config",
    "flagdash",
    "feature-toggles",
    "sdk"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flagdash/flagdash-js.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://flagdash.com",
  "bugs": {
    "url": "https://github.com/flagdash/flagdash-js/issues"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
