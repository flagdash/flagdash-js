{
  "name": "@flagdash/react-native",
  "version": "0.1.0",
  "description": "FlagDash React Native SDK â€” feature flags, remote config, and AI configs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external react-native --external @react-native-async-storage/async-storage",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --external react-native --external @react-native-async-storage/async-storage --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "feature-flags",
    "remote-config",
    "ai-config",
    "flagdash",
    "react-native",
    "mobile",
    "offline",
    "async-storage"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flagdash/flagdash-js.git",
    "directory": "packages/react-native"
  },
  "homepage": "https://flagdash.com",
  "bugs": {
    "url": "https://github.com/flagdash/flagdash-js/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.71.0",
    "@react-native-async-storage/async-storage": ">=1.19.0"
  },
  "dependencies": {
    "@flagdash/sdk": "workspace:*"
  },
  "devDependencies": {
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.2.0",
    "jsdom": "^28.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}

